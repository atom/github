{
  "name": "git",
  "main": "./lib/main",
  "version": "0.0.0",
  "description": "Git things",
  "repository": "https://github.com/atom/git",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "scripts": {
    "lint": "./node_modules/.bin/standard lib/**/*.js spec/**/*.js",
    "check": "./node_modules/.bin/flow check",
    "start": "./node_modules/.bin/flow",
    "stop": "./node_modules/.bin/flow stop"
  },
  "dependencies": {
    "diff": "^2.2.1",
    "dom-listener": "0.1.2",
    "etch": "0.3.0",
    "nan": "2.x",
    "pathwatcher": "~6.2"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.8",
    "flow-bin": "^0.22.0",
    "fs-plus": "^2.8.1",
    "standard": "^5.4.1",
    "temp": "^0.8.3"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "github": {
      "versions": {
        "^0.0.1": "consumeGitHubToken"
      }
    }
  },
  "providedServices": {
    "git-push": {
      "description": "A function which can be called to push the current branch.",
      "versions": {
        "0.0.1": "provideGitPush",
      }
    }
  },
  "standard": {
    "ignore": [],
    "parser": "babel-eslint",
    "globals": [
      "atom",
      "afterEach",
      "beforeEach",
      "describe",
      "fdescribe",
      "xdescribe",
      "expect",
      "it",
      "fit",
      "xit",
      "jasmine",
      "runs",
      "spyOn",
      "waitsFor",
      "MouseEvent",
      "localStorage",
      "HTMLElement",
      "Event",
      "EventTarget"
    ]
  }
}
