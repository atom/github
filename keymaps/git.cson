'atom-workspace':
  'cmd-shift-c': 'git:toggle-git-panel'

'.git-StagingView.staged-changes-focused':
  'tab': 'git:focus-unstaged-changes'

'.git-StagingView.unstaged-changes-focused':
  'tab': 'git:focus-staged-changes'

'.git-CommitView-editor atom-text-editor:not([mini])':
  'cmd-enter': 'git:commit'
  'ctrl-enter': 'git:commit'

'.git-FilePatchView':
  '/': 'git:toggle-patch-selection-mode'
  'tab': 'git:focus-next-hunk'
